@model Slider

@{
    ViewData["Title"] = "Delete";
}
<form method="post">
    <div>
        <label for="title" class="form-label">Slider Title</label>
        <input asp-for="Title" class="form-control border border-info" id="title" aria-describedby="emailHelp"/>
        <span asp-validation-for="Title" class="text-danger"></span>
    </div>

   <button type="submit" class="mt-3 btn btn-danger">Delete</button>
</form>

