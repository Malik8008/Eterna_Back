                        <ul class="mail d-flex">
                            <li><i class="@layoutserice.GetDatas().Result.FirstOrDefault(p=>p.Key=="EmailIcon").Value"></i></li>
                            <li><a href="">@layoutserice.GetDatas().Result.FirstOrDefault(p=>p.Key=="Email").Value</a></li>
                        </ul>
                        <ul class="tel d-flex">
                            <li><i class="@layoutserice.GetDatas().Result.FirstOrDefault(p=>p.Key=="TelephoneIcon").Value"></i></li>
                            <li>@layoutserice.GetDatas().Result.FirstOrDefault(p=>p.Key=="Telephone").Value</li>
                        </ul>
                    </div>
                    <div class="col-lg-8 social_media mt-2">
                        <ul class="social d-flex justify-content-end">
